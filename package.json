{
  "name": "ai-chatboat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "build:frontend": "vite build --config frontend/vite.config.ts",
    "preview:frontend": "vite preview --config frontend/vite.config.ts",
    "dev:all": "concurrently -k -n API,WEB -c \"cyan,magenta\" \"npm run dev\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/openai": "^3.0.26",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^8.2.0",
    "ai": "^6.0.77",
    "dotenv": "^16.4.5",
    "fastify": "^5.7.4",
    "mysql2": "^3.10.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  }
}
